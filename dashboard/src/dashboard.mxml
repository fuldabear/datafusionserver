										</mx:FormItem>
										<mx:FormItem label="Session Password">
											<mx:TextInput displayAsPassword="true" id="session_password_name" creationComplete="{this.session_password_password.text = this.user_password.text; updateDebugSessionName(event);}" change="updateDebugSessionName(event);" click="updateDebugSessionName(event);"/>
										</mx:FormItem>
										<mx:FormHeading label="New Session Properties"/>
										<mx:FormItem label="Name">
											<mx:TextInput id="session_newname_name" change="updateDebugSessionName(event);" click="updateDebugSessionName(event);"/>
										</mx:FormItem>
