	<mx:Script source="dashboard.as"/>
	
    <!-- Define one view state, in addition to the base state.-->
    <mx:states>
        <mx:State name="App" id="appState">
            <mx:RemoveChild target="{loginPanel}"/>
            <mx:RemoveChild target="{spacer2}"/>
            <mx:SetProperty name="layout" value="absolute"/>
            <mx:SetStyle target="{image1}" name="verticalCenter" value="0"/>
            <mx:SetStyle target="{image1}" name="horizontalCenter" value="0"/>
            <mx:AddChild relativeTo="{image1}" position="before">
                <mx:MenuBar id="topMenuBar" labelField="@label" top="0" left="0" width="100%" itemClick="menuHandler2(event);">
					<mx:XMLList>
						<menuitem label="Applications">
							<menuitem label="Session Manager" data="session"/>
							<menuitem label="Data Manager" data="data"/>
							<menuitem label="Function Manager" data="function"/>
						</menuitem>
						<menuitem label="Help">
							<menuitem label="Debug" data="debug"/>
							<menuitem label="About" data="about"/>
						</menuitem>
					</mx:XMLList>
				</mx:MenuBar>
            </mx:AddChild>
            <mx:AddChild position="lastChild">
                <!--<mx:ModuleLoader id="mainMod" left="10" right="10" bottom="10" top="30" />-->
            </mx:AddChild>
            <mx:AddChild position="lastChild">
                <mx:TextArea bottom="5" left="10" right="10" height="20" text="Login Successful" id="statusBar" fontSize="11"/>
            </mx:AddChild>
        </mx:State>
        <mx:State name="session" basedOn="App">
            <mx:AddChild position="lastChild">
                <mx:Panel layout="absolute" top="30" left="10" bottom="30" right="10" title="Session Manager">
                    <mx:HBox top="35" left="5" right="5" bottom="5">
                        <mx:VBox width="33%" height="100%">
                            <mx:DataGrid height="100%" width="100%">
                                <mx:columns>
                                    <mx:DataGridColumn headerText="Name" dataField="name"/>
                                    <mx:DataGridColumn headerText="Variables" dataField="varaibles"/>
                                    <mx:DataGridColumn headerText="Functions" dataField="functions"/>
									<mx:DataGridColumn headerText="Expiration" dataField="expiration"/>
                                </mx:columns>
                            </mx:DataGrid>
                            <mx:HBox width="100%" height="35" verticalAlign="middle">
								<mx:Spacer/>
								<mx:Button label="Delete"/>
								<mx:Spacer/>
								<mx:VRule height="100%"/>
								<mx:Spacer width="100%"/>
                                <mx:Button label="Duplicate"/>
                                <mx:Spacer/>
                                <mx:Button label="New"/>
								<mx:Spacer/>
                            </mx:HBox>
                        </mx:VBox>
                        <mx:VBox width="66%" height="100%">
                            <mx:Form width="100%" height="100%">
                                <mx:FormHeading label="Authentication" width="100%"/>
                                <mx:FormItem label="Session Name" width="100%">
                                    <mx:TextInput/>
                                </mx:FormItem>
								<mx:FormItem label="Session Password" width="100%">
									<mx:TextInput displayAsPassword="true"/>
								</mx:FormItem>
								<mx:FormHeading label="Properties" width="100%"/>
								<mx:FormItem label="Expiration" width="100%">
									<mx:HBox>
										<mx:DateField x="129" y="1"/>
									</mx:HBox>
								</mx:FormItem>
								<mx:FormItem label="Description" width="100%">
									<mx:TextInput width="100%" height="150"/>
								</mx:FormItem>
								<mx:FormHeading label="Statistics" width="100%"/>
								<mx:FormItem label="Variables" width="100%" height="150">
									<mx:HBox width="100%" height="100%">
										<mx:PieChart id="piechart0" width="50%" height="100%">
											<mx:series>
												<mx:PieSeries displayName="Series 1" field=""/>
											</mx:series>
										</mx:PieChart>
										<mx:Legend dataProvider="{piechart0}"/>
									</mx:HBox>
								</mx:FormItem>
								<mx:FormItem label="Functions" width="100%" height="150">
									<mx:HBox width="100%" height="100%">
										<mx:PieChart id="piechart1" width="50%" height="100%">
											<mx:series>
												<mx:PieSeries displayName="Series 1" field=""/>
											</mx:series>
										</mx:PieChart>
										<mx:Legend dataProvider="{piechart1}"/>
									</mx:HBox>
								</mx:FormItem>
                            </mx:Form>
                        </mx:VBox>
                    </mx:HBox>
                </mx:Panel>
            </mx:AddChild>
        </mx:State>
