										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click>{this.inSQL.text = &quot;select &quot;+this.what.text+&quot; from &quot;+this.table3.text; if(this.where2.text != &quot;&quot;) this.inSQL.text += &quot; where &quot;+this.where2.text; applyButton();}</mx:click>
										            </mx:Button>
										        </mx:FormItem>
