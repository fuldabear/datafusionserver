										            <mx:Button 
										                label="Insert"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><cdata>"+this.controlDataSet.text+"</cdata><user_id>"+this.userId.text+"</user_id><simulation_id>"+this.simulationId.text+"</simulation_id>"; if (dataId.text != "") inDashML.text += "<data_id>"+this.dataId2.text+"</data_id>"; inDashML.text += "</dashML>";}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><cdata>"+this.controlDataSet.text+"</cdata><user_id>"+this.userId.text+"</user_id><simulation_id>"+this.simulationId.text+"</simulation_id>"; if (dataId.text != "") inDashML.text += "<data_id>"+this.dataId2.text+"</data_id>"; inDashML.text += "</dashML>"; applyButton();}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
                                        </mx:Canvas>
