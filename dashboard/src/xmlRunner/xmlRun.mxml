										                <mx:click><![CDATA[{inDashML.text = "<dashML><config_data>"+this.configurationDataSet.text+"</config_data><dash_id>"+this.dashId3.text+"</dash_id><simulation_id>"+this.simulationId2.text+"</simulation_id></dashML>";}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><config_data>"+this.configurationDataSet.text+"</config_data><dash_id>"+this.dashId3.text+"</dash_id><simulation_id>"+this.simulationId2.text+"</simulation_id></dashML>"; applyButton();}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
                                        </mx:Canvas>
