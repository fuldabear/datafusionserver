                                        	<mx:Form top="10" left="10" right="16" bottom="10">
                                            	<mx:FormItem label="Into Table:" width="100%">
										            <mx:TextArea id="table2"  width="100%" wordWrap="true"/>
										        </mx:FormItem>									
										        <mx:FormItem label="column1=expr1[,[column2=expr2]...]:" width="100%">
										            <mx:TextArea id="expr"  width="100%" wordWrap="true"/>
										        </mx:FormItem>
										        <mx:FormItem label="Returns:">
										            <mx:Label text="Status"/>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert"  width="108">
										                <mx:click>{this.inSQL.text = &quot;insert into &quot;+this.table2.text+&quot; set &quot;+this.expr.text;}</mx:click>
										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click>{this.inSQL.text = &quot;insert into &quot;+this.table2.text+&quot; set &quot;+this.expr.text; applyButton();}</mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
