										            <mx:TextArea id="simulationId2" width="100%" wordWrap="true"/>
										        </mx:FormItem>
										        <mx:FormItem label="Configuration Data Set:" width="100%">
										            <mx:TextArea id="configurationDataSet" width="100%" wordWrap="true"/>
