										                <mx:click><![CDATA[{inDashML.text = "<dashML><sql><query>"+this.sqlIn.text+"</query></sql></dashML>";}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><sql><query>"+this.sqlIn.text+"</query></sql></dashML>"; applyButton();}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
                                        </mx:Canvas>
                                        <mx:Canvas label="Repeater" width="100%" height="100%">
                                        	<mx:Form top="10" left="10" right="16" bottom="10">
                                        		<mx:FormItem label="Any:" width="100%">
										            <mx:TextArea id="any" width="100%" wordWrap="true"/>
                                        		</mx:FormItem>
                                        		<mx:FormItem label="Returns:">
										            <mx:Label text="Any"/>
                                        		</mx:FormItem>
                                        		<mx:FormItem>
										            <mx:Button 
										                label="Insert"  width="108">
										            	<mx:click><![CDATA[{inDashML.text = "<dashML><repeater><string>"+this.any.text+"</string></repeater></dashML>";}]]></mx:click>
										            </mx:Button>
                                        		</mx:FormItem>
                                        		<mx:FormItem>
                                        		    <mx:Button 
                                        		        label="Insert / Apply"  width="108">
                                        		        <mx:click><![CDATA[{inDashML.text = "<dashML><repeater><string>"+this.any.text+"</string></repeater></dashML>"; applyButton();}]]></mx:click>
                                        		    </mx:Button>
                                        		</mx:FormItem>
                                            </mx:Form>
                                        </mx:Canvas>
                                        <mx:Canvas label="IP" width="100%" height="100%">
