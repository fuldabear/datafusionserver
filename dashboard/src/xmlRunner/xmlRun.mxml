                    <mx:Canvas width="100%" height="100%">
                        <mx:VBox x="0" y="0" width="100%" height="100%">
                            <mx:Canvas width="100%" height="100%">
                                <mx:Panel layout="absolute" title="SQL Syntax" top="10" left="10" bottom="10" right="0">
                                    <mx:Accordion top="0" left="0" right="0" bottom="0">
