										            <mx:Label text="The Number of Delete Rows"/>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert"  width="108" click="{this.inSQL.text = &quot;delete from &quot;+this.table.text; if(this.where.text != &quot;&quot;) this.inSQL.text += &quot; where &quot;+this.where.text;}"/>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click>{this.inSQL.text = &quot;delete from &quot;+this.table.text; if(this.where.text != &quot;&quot;) this.inSQL.text += &quot; where &quot;+this.where.text; applyButton();}</mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
