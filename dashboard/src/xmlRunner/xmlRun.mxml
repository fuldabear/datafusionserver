										            <mx:Button 
										                label="Insert"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><data>"+this.data.text+"</data>"; if (dataId.text != "") inDashML.text += "<data_id>"+this.dataId.text+"</data_id>"; inDashML.text += "</dashML>";}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
										        <mx:FormItem>
										            <mx:Button 
										                label="Insert / Apply"  width="108">
										                <mx:click><![CDATA[{inDashML.text = "<dashML><data>"+this.data.text+"</data>"; if (dataId.text != "") inDashML.text += "<data_id>"+this.dataId.text+"</data_id>"; inDashML.text += "</dashML>"; applyButton();}]]></mx:click>
										            </mx:Button>
										        </mx:FormItem>
                                            </mx:Form>
                                        </mx:Canvas>
                                        <mx:Canvas label="Control Data" width="100%" height="100%">
