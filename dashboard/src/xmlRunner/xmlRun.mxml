										            <mx:TextArea id="table3"  width="100%" wordWrap="true"/>
										        </mx:FormItem>									
										        <mx:FormItem label="[Where]:" width="100%">
										            <mx:TextArea id="where2"  width="100%" wordWrap="true"/>
