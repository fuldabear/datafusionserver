                                            	<mx:FormItem label="From Table:" width="100%">
										            <mx:TextArea id="table"  width="100%" wordWrap="true"/>
										        </mx:FormItem>									
										        <mx:FormItem label="[Where]:" width="100%">
										            <mx:TextArea id="where"  width="100%" wordWrap="true"/>
